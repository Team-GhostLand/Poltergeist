generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider = "sqlite"
    url      = env("DATABASE_URL")
}

model whitelist {
    uuid         String @id //varchar(60)
    name         String //varchar(255)
    whitelisted  Int //tinyint
    
    dcuser       users?
}

model users {
    discordsnowflakeid  String @id
    mcuuid              String @unique
    mcname              String @unique
    
    mcuser_whlist_entry whitelist       @relation(fields:[mcuuid], references: [uuid])
  //mcuser_account      minecraftusers? @relation(fields:[mcname], references: [username])
}

model minecraftusers {
    id             Int @id
    username       String @unique //varchar(255)
    username_lower String //varchar(255)
    uuid           String? //varchar(255)
    data           String //json
    
  //dcuser         users?
}